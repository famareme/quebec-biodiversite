{
    "title": "Landing Page",
    "name": "startbootstrap-landing-page",
    "version": "6.0.6",
    "scripts": {
        "build": "pug views/ --out dist/ --pretty",
        "build:assets": "node scripts/build-assets.js",
        "build:views": "node scripts/build-pug.js",
        "build:scripts": "node scripts/build-scripts.js",
        "build:scss": "node scripts/build-scss.js",
        "clean": "node scripts/clean.js",
        "start": "node server.js",
        "start:debug": "npm run build && node scripts/start-debug.js"
    },
    "description": "An HTML landing page template built with Bootstrap",
    "keywords": [
        "css",
        "sass",
        "html",
        "responsive",
        "theme",
        "template"
    ],
    "homepage": "https://startbootstrap.com/theme/landing-page",
    "bugs": {
        "url": "https://github.com/StartBootstrap/startbootstrap-landing-page/issues",
        "email": "feedback@startbootstrap.com"
    },
    "license": "MIT",
    "author": "Start Bootstrap",
    "contributors": [
        "David Miller (https://davidmiller.io/)"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/StartBootstrap/startbootstrap-landing-page.git"
    },
    "dependencies": {
        "bootstrap": "5.2.3",
        "express": "^4.21.2"
    },
    "devDependencies": {
        "autoprefixer": "10.4.14",
        "browser-sync": "^3.0.3",
        "chokidar": "3.5.3",
        "concurrently": "6.3.0",
        "postcss": "^8.4.49",
        "prettier": "2.8.6",
        "pug": "^3.0.3",
        "sass": "1.60.0",
        "shelljs": "0.8.5",
        "upath": "2.0.1"
    }
}
